<?xml version="1.0" encoding="utf-8" ?>
<mopups:PopupPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:mopups="clr-namespace:Mopups.Pages;assembly=Mopups"
             x:Class="HonorsApplication_II.PopupPages.ExtraInfoPopup" 
             CloseWhenBackgroundIsClicked="true" BackgroundColor="#80000000">


    <ScrollView VerticalOptions="Center" >

        <Frame BackgroundColor="LightGrey" Padding="0,5" >

            <Label x:Name="TextLabel" FontSize="Medium" Margin="30"/>



        </Frame>

    </ScrollView>
</mopups:PopupPage>
